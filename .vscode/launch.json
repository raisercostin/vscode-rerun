{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run Extension (proposed API enabled)",
      "type": "extensionHost",
      "request": "launch",
      "runtimeExecutable": "${execPath}",
      "args": [
        "--extensionDevelopmentPath=${workspaceFolder}",
        "--enable-proposed-api",
        "raisercostin.rerun"
      ]
    }
  ]
}
